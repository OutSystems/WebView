<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<config>
        <add key="repositoryPath" value="packages" />
        <add key="globalPackagesFolder" value="packages" />
    </config>
	<packageRestore>
		<!-- Opt-In both Package Restore, and MSBuild-Integrated Package Restore, does not make MSBuild restore automatic -->
		<add key="enabled" value="True" />
		<!-- Opt-In Automatic Package Restore in Visual Studio -->
		<add key="automatic" value="True" />
	</packageRestore>
  <packageSources>
    <add key="Azure" value="https://pkgs.dev.azure.com/OutSystemsRD/_packaging/ArtifactRepository/nuget/v3/index.json"/>
  </packageSources>
</configuration>

